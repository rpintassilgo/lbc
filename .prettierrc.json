{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "tabWidth": 2,
  "printWidth": 120,
  "semi": true,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "semi": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "singleQuote": true
      }
    }
  ]
}
